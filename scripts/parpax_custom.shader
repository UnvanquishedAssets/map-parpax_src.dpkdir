textures/parpax_custom/squarelamp_white_10k
{
	surfaceparm nomarks
	q3map_surfacelight 10000
	q3map_lightimage textures/parpax_custom/squarelamp_white.blend.png
	qer_editorimage textures/parpax_custom/squarelamp_white.png

	{
		map $lightmap
		rgbGen identity
	}
	{
		map textures/parpax_custom/squarelamp_white.png
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
	{
		map textures/parpax_custom/squarelamp_white.blend.png
		blendfunc GL_ONE GL_ONE
	}
}

